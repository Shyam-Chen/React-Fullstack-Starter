{
  "name": "react-play",
  "version": "1.0.0",
  "description": "A boilerplate for React, Material, Express, Babel, Flow, and ReactiveX.",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "start": "cross-env BABEL_ENV=app webpack-dev-server --progress",
    "build": "rimraf public functions && cross-env BABEL_ENV=app webpack --env.prod && cross-env BABEL_ENV=api gulp --prod",
    "lint": "eslint src/client.js 'src/app/**/*.js'",
    "unit": "cross-env BABEL_ENV=app JEST_ENV=app jest \"src/app/.*\\.js$\" --coverage --forceExit",
    "e2e": "cross-env BABEL_ENV=test JEST_ENV=e2e jest \"src/app/.*\\.js$\" --forceExit",
    "typed": "flow-typed install",
    "codecov": "codecov",
    "firebase": "firebase",
    "flow-typed": "flow-typed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/React-Play.git"
  },
  "keywords": [
    "React",
    "Fullstack"
  ],
  "author": "Shyam Chen",
  "license": "MIT",
  "bugs": {
